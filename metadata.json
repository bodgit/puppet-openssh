{
  "name": "bodgit-openssh",
  "version": "1.0.1",
  "author": "bodgit",
  "summary": "Puppet Module for managing OpenSSH",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-openssh",
  "project_page": "https://github.com/bodgit/puppet-openssh",
  "issues_url": "https://github.com/bodgit/puppet-openssh/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "OpenBSD",
      "operatingsystemrelease": [
        "6.2",
        "6.3"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.9.0 <7.0.0"
    }
  ],
  "tags": [
    "openssh",
    "ssh"
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">=4.13.0 <5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">=2.1.0 <5.0.0"
    },
    {
      "name": "bodgit/bodgitlib",
      "version_requirement": ">=2.0.1 <3.0.0"
    },
    {
      "name": "thrnio/ip",
      "version_requirement": ">=1.0.0 <2.0.0"
    }
  ]
}
